@{
    ViewData["Title"] = "Detail Dokter";
    Layout = "~/Views/Shared/_TopNavbarHealthManagementDoctor.cshtml";
    var photoPath = "~/FotoDokter/" + (Model.DoctorPhotoPath ?? "doctor.jpg");
}

@using BenariMikronWebApp.Areas.Identity.Data;
@using Microsoft.AspNetCore.Identity;
@using BenariMikronWebApp.Areas.HealthManagement.Models;

@inject Microsoft.AspNetCore.Identity.SignInManager<ApplicationUser> signInManager
@model DetailDoctorViewModel

<!-- Tempusdominus Bootstrap 4 -->
<link rel="stylesheet" href="~/admin/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-3">

            <!-- Profile Image -->
            <div class="card card-primary card-outline">
                <div class="card-body box-profile">
                    <div class="text-center">
                        <img class="profile-user-img img-fluid img-circle img-bordered-sm"
                             src="@photoPath" asp-append-version="true" />
                    </div>

                    <h3 class="profile-username text-center">@Html.DisplayFor(model => model.NamaLengkap)</h3>

                    <p class="text-muted text-center">@Html.DisplayFor(model => model.NamaMarga)</p>

                    <ul class="list-group list-group-unbordered mb-3">
                        <li class="list-group-item">
                            <b>Kode Dokter</b> <a class="float-right">@Html.DisplayFor(model => model.KodeDokter)</a>
                        </li>
                        <li class="list-group-item">
                            <b>Tipe Dokter</b> <a class="float-right">@Html.DisplayFor(model => model.DoctorType)</a>
                        </li>
                        <li class="list-group-item">
                            <b>No. Ktp Dokter</b> <a class="float-right">@Html.DisplayFor(model => model.NomorKtpDokter)</a>
                        </li>
                    </ul>

                    @*<a href="#" class="btn btn-primary btn-block"><b>Follow</b></a>*@
                    <div class="card-footer">
                        <button type="submit" class="btn btn-primary bg-orange float-left fw-bold"><div style="color:#ffffff">Ubah</div></button>
                        <a class="btn btn-danger" asp-action="DeleteDoctor" asp-controller="Doctor" asp-area="HealthManagement" asp-route-id="@Model.DoctorId">Hapus</a>
                        <a class="btn btn-default" asp-action="Index" asp-controller="Doctor" asp-area="HealthManagement">Batal</a>
                    </div>
                </div>
                <!-- /.card-body -->
            </div>
        </div>
