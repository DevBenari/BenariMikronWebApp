                                <div class="col-sm-9 input-group date" id="tanggalKeluar" data-target-input="nearest">
                                    <div class="input-group-append" data-target="#tanggalKeluar" data-toggle="datetimepicker">
                                        <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                                    </div>
                                    <input asp-for="TanggalKeluar" type="text" class="form-control datetimepicker-input" data-target="#tanggalKeluar" placeholder="Tanggal Keluar" />
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" asp-for="GuaranteeFee">Guarantee Fee</label>
                                <div class="col-sm-9">
                                    <input maxlength="100" type="text" asp-for="GuaranteeFee" class="form-control form-control-border border-width-2" id="" placeholder="Guarantee Fee">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" asp-for="DokterMitra">Dokter Mitra (*)</label>
                                <div class="col-sm-9">
                                    <select asp-for="DokterMitra" class="form-control select2bs4" style="width: 100%;">
                                        <option value="" hidden disabled selected>--Pilih--</option>
                                        <option value="Ya">Ya</option>
                                        <option value="Tidak">Tidak</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" asp-for="DokterSpesialis">Dokter Spesialis (*)</label>
                                <div class="col-sm-9">
                                    <select asp-for="DokterSpesialis" class="form-control select2bs4" style="width: 100%;">
                                        <option value="" hidden disabled selected>--Pilih--</option>
                                        <option value="Ya">Ya</option>
                                        <option value="Tidak">Tidak</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" asp-for="DepartmentList">
                                    Departemen (*) <small class="badge bg-orange" id="toolDepartemen"
                                                          data-toggle="tooltip"
                                                          data-placement="top"
                                                          title="Data : Manajemen Kesehatan > Master Data > Dokter > Departemen Dokter"><i class="fas fa-info-circle" style="color:white"></i></small>
                                </label>
                                <div class="col-sm-9">
                                    <select asp-for="DepartmentId" asp-items="Model.DepartmentList" multiple="multiple" data-placeholder="--Pilih Departemen--" class="form-control select2bs4" style="width: 100%;">
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" asp-for="Foto">Foto</label>
                                <div class="col-sm-9">
                                    <div class="col-sm-9">
                                        <img src="@photoPath" asp-append-version="true" width="300" height="300" />
                                    </div>
                                    <div class="col-sm-9">
                                        <div class="custom-file">
                                            <input asp-for="Foto" class="form-control custom-file-input">
                                            <label class="custom-file-label">Ganti Foto...</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- start button footer -->
                        <div class="card card-orange">
                            <div class="card-body">
                                <button type="submit" class="btn btn-primary bg-orange float-left fw-bold"><div style="color:#ffffff">Simpan</div></button>
                                <a class="btn btn-danger" asp-action="DeleteDoctor" asp-controller="Doctor" asp-area="HealthManagement" asp-route-id="@Model.DoctorId">Hapus</a>
                                <a class="btn btn-default" asp-action="Index" asp-controller="Doctor" asp-area="HealthManagement">Batal</a>
                            </div>
                        </div>
                        <!-- /.end button footer -->
                        @section Scripts {
                            <script>
                                $(function () {
                                    //Initialize Select2 Elements
                                    $('.select2bs4').select2({
                                        theme: 'bootstrap4'
                                    })
                                    $('#toolTipeDokter').tooltip({ animation: true });
                                    $('#toolPendidikanTerakhir').tooltip({ animation: true });
                                    $('#toolAgama').tooltip({ animation: true });
                                    $('#toolPekerjaan').tooltip({ animation: true });
                                    $('#toolTipeAntrianDokter').tooltip({ animation: true });
                                    $('#toolBank').tooltip({ animation: true });
                                    $('#toolBankCabang').tooltip({ animation: true });
                                    $('#toolNegara').tooltip({ animation: true });
                                    $('#toolProvinsi').tooltip({ animation: true });
                                    $('#toolKota').tooltip({ animation: true });
                                    $('#toolKecamatan').tooltip({ animation: true });
                                    $('#toolKelurahan').tooltip({ animation: true });
                                    $('#toolGelarDokter').tooltip({ animation: true });
                                    $('#toolDepartemen').tooltip({ animation: true });
                                    $('#toolKewarganegaraan').tooltip({ animation: true });
                                })
                            </script>

                            <script>
                                $(document).ready(function () {
                                    $('.custom-file-input').on("change", function () {
                                        var fileName = $(this).val().split("\\").pop();
                                        $(this).next('.custom-file-label').html(fileName);
                                    });
                                });
                            </script>

                            <script>
                                $(document).ready(function () {
                                    //$("#ProvinceList").empty().append('<option value = "' + -1 + '">' + "--Pilih Provinsi--" + '</option>');
                                    //$("#CityList").empty().append('<option value = "' + -1 + '">' + "--Pilih Kota--" + '</option>');
                                    //$("#DistrictList").empty().append('<option value = "' + -1 + '">' + "--Pilih Kecamatan--" + '</option>');
                                    //$("#SubDistrictList").empty().append('<option value = "' + -1 + '">' + "--Pilih Kelurahan--" + '</option>');

                                    $("#CountryList").change(function () {
                                        //$("#ProvinceList").empty();
                                        $.ajax({
                                            type: 'GET',
                                            url: '@Url.Action("LoadProvince")',
                                            dataType: 'json',
                                            data: { id: $("#CountryList").val() },
                                            success: function (provinces) {
                                                $("#ProvinceList").append('<option value = "' + -1 + '">' + "--Pilih Provinsi--" + '</option>');
                                                $.each(provinces, function (i, province) {
                                                    $("#ProvinceList").append('<option value = "' + province.value + '">' + province.text + '</option>');
                                                });
                                            },
                                            error: function (ex) {
                                                alert('Gagal ambil data provinsi' + ex);
                                            }
                                        });
                                        return false;
                                    })

                                    $("#ProvinceList").change(function () {
                                        //$("#CityList").empty();
                                        $.ajax({
                                            type: 'GET',
                                            url: '@Url.Action("LoadCity")',
                                            dataType: 'json',
                                            data: { id: $("#ProvinceList").val() },
                                            success: function (cities) {
                                                $("#CityList").append('<option value="' + -1 + '">' + "--Pilih Kota--" + '</option>');
                                                $.each(cities, function (i, city) {
                                                    $("#CityList").append('<option value="' + city.value + '">' + city.text + '</option>');
                                                });
                                            },
                                            error: function (ex) {
                                                alert('Gagal ambil data kota' + ex);
                                            }
                                        });
                                        return false;
                                    })

                                    $("#CityList").change(function () {
                                        //$("#DistrictList").empty();
                                        $.ajax({
                                            type: 'GET',
                                            url: '@Url.Action("LoadDistrict")',
                                            dataType: 'json',
                                            data: { id: $("#CityList").val() },
                                            success: function (districts) {
                                                $("#DistrictList").append('<option value="' + -1 + '">' + "--Pilih Kecamatan--" + '</option>');
                                                $.each(districts, function (i, district) {
                                                    $("#DistrictList").append('<option value="' + district.value + '">' + district.text + '</option>');
                                                });
                                            },
                                            error: function (ex) {
                                                alert('Gagal ambil data kecamatan' + ex);
                                            }
                                        });
                                        return false;
                                    })

                                    $("#DistrictList").change(function () {
                                        //$("#SubDistrictList").empty();
                                        $.ajax({
                                            type: 'GET',
                                            url: '@Url.Action("LoadSubDistrict")',
                                            dataType: 'json',
                                            data: { id: $("#DistrictList").val() },
                                            success: function (subdistricts) {
                                                $("#SubDistrictList").append('<option value="' + -1 + '">' + "--Pilih Kelurahan--" + '</option>');
                                                $.each(subdistricts, function (i, subdistrict) {
                                                    $("#SubDistrictList").append('<option value="' + subdistrict.value + '">' + subdistrict.text + '</option>');
                                                });
                                            },
                                            error: function (ex) {
                                                alert('Gagal ambil data kelurahan' + ex);
                                            }
                                        });
                                        return false;
                                    })
                                })
                            </script>

                            <script>
                                $(document).ready(function () {
                                    //$("#BankCabangList").empty().append('<option value = "' + -1 + '">' + "--Pilih Bank Cabang--" + '</option>');

                                    $("#BankList").change(function () {
                                        $("#BankCabangList").empty();
                                        $.ajax({
                                            type: 'GET',
                                            url: '@Url.Action("LoadBankCabang")',
                                            dataType: 'json',
                                            data: { id: $("#BankList").val() },
                                            success: function (cabangs) {
                                                $("#BankCabangList").append('<option value = "' + -1 + '">' + "--Pilih Bank Cabang--" + '</option>');
                                                $.each(cabangs, function (i, bcb) {
                                                    $("#BankCabangList").append('<option value = "' + bcb.value + '">' + bcb.text + '</option>');
                                                });
                                            },
                                            error: function (ex) {
                                                alert('Gagal ambil data Bank Cabang' + ex);
                                            }
                                        });
                                        return false;
                                    })
                                })
                            </script>

                            <script>
                                $(function () {
                                    //Date picker
                                    $('#tanggallahirdokter').datetimepicker({
                                        format: 'L'
                                    })

                                    //Data-Mask-Ktp-Dokter
                                    $('[data-mask-ktp-dokter]').inputmask()

                                    //Data-Mask-Npwp
                                    $('[data-mask-npwp]').inputmask()

                                    //Data-Mask-Hp
                                    $('[data-mask-hp]').inputmask()

                                    //Data-Mask-Telepon
                                    $('[data-mask-telepon]').inputmask()

                                    //Data-Mask-Telepon-Kantor
                                    $('[data-mask-telepon-kantor]').inputmask()

                                    //Tanggal Awal Kontrak
                                    $('#tanggalAwalKontrak').datetimepicker({
                                        format: 'L'
                                    })

                                    //Tanggal Akhir Kontrak
                                    $('#tanggalAkhirKontrak').datetimepicker({
                                        format: 'L'
                                    })

                                    //Tanggal Keluar
                                    $('#tanggalKeluar').datetimepicker({
                                        format: 'L'
                                    })
                                })
                            </script>
                        }
                    </form>
                </div><!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->